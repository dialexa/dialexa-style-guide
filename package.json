{
  "name": "@dialexa/style-kit",
  "version": "1.3.2",
  "description": "CSS/ SASS Style kit to set up Dialexa projects",
  "scripts": {
    "css:build": "node-sass --output-style=compact --include-path style-kit/ style-kit/app.scss dist/dialexa-style-kit.css",
    "css:prefix": "postcss --use autoprefixer -b 'last 2 versions' dist/*.css -d dist/",
    "build-dist": "npm run css:build && npm run css:prefix"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.1",
    "node-sass": "^3.4.2",
    "postcss-cli": "^2.5.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dialexa/style-kit.git"
  },
  "keywords": [
    "sass",
    "css",
    "style-kit",
    "dialexa-ui",
    "dialexa"
  ],
  "author": "Dialexa",
  "contributors": [
    "Chris Miller <chris@dialexa.com>"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dialexa/style-kit/issues"
  },
  "homepage": "https://github.com/dialexa/style-kit#readme"
}
