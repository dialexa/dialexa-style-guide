{
  "name": "@dialexa/dialexa-style-guide",
  "version": "0.2.0",
  "description": "CSS/ SASS Style base for Dialexa projects",
  "scripts": {
    "css:build": "node-sass --include-path style-guide/ style-guide/app.scss dist/dialexa-style-guide.css",
    "css:prefix": "postcss --use autoprefixer -b 'last 2 versions' dist/*.css -d dist/",
    "build-dist": "npm run css:build && npm run css:prefix"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.1",
    "node-sass": "^3.4.2",
    "postcss-cli": "^2.5.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dialexa/dialexa-style-guide.git"
  },
  "keywords": [
    "sass",
    "css",
    "style-guide",
    "dialexa"
  ],
  "author": "Chris Miller <chris@dialexa.com> (http://dialexa.com)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dialexa/dialexa-style-guide/issues"
  },
  "homepage": "https://github.com/dialexa/dialexa-style-guide#readme",
  "main": "index.js"
}
